<%#
#kind: PXELinux
#name: WAIK default PXELinux
#oses:
#- Windows 6.1
#- Windows 6.3
-%>
<% pxe_subpath = "boot/windows-x64" -%>
DEFAULT menu    
LABEL menu         
     COM32 linux.c32 <%= pxe_subpath -%>/wimboot
     APPEND initrdfile=<%= pxe_subpath -%>/bcd,<%= pxe_subpath -%>/boot.sdi,<%= pxe_subpath -%>/boot.wim
